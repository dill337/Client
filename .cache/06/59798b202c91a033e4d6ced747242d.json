onents/movie-card/movie-card.jsx":"import React from 'react';\nimport PropTypes from 'prop-types';\nimport { Button, Card } from 'react-bootstrap';\n\nimport { Link } from \"react-router-dom\";\n\nimport \"./movie-card.scss\"\n\nexport class MovieCard extends React.Component {\n  render() {\n    const { movie, onClick } = this.props;\n\n    return (\n      <Card style={{ width: '16rem' }}>\n        <Card.Img variant=\"top\" src={movie.ImagePath} style={{ width: '140px' }} />\n        <br></br>\n        <Card.Body>\n          <Card.Title>{movie.Title}</Card.Title>\n          <Card.Text>{movie.Description}</Card.Text>\n          <Link to={`/movies/${movie._id}`}>\n            <Button onClick={() => this.props.onMovieClick(movie)} variant=\"link\">Read More</Button>\n          </Link>\n          <br></br>\n          <br></br>\n          <br></br>\n          <br></br>\n        </Card.Body>\n      </Card >\n    );\n  }\n}\nMovieCard.propTypes = {\n  movie: PropTypes.shape({\n    Title: PropTypes.string.isRequired,\n    Description: PropTypes.string.isRequired,\n    ImagePath: PropTypes.string.isRequired\n  }).isRequired,\n  //onClick: PropTypes.func.isRequired\n};"},"lineCount":null}},"error":null,"hash":"b62014bfb108a06d2f735247c000eccb","cacheData":{"env":{}}}